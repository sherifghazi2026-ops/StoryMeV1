{
  "expo": {
    "name": "StoryMe",
    "slug": "storyme",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "platforms": [
      "ios",
      "android"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.storyme.app",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "plugins": [
      "expo-image-picker",
      "expo-av",
      "expo-audio"
    ],
    "extra": {
      "eas": {
        "projectId": "28f2fc14-4d34-479b-ae5b-d6ff2e2a619f"
      }
    }
  }
}
